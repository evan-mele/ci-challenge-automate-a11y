{
  "name": "simple-react-boilerplate",
  "version": "1.0.0",
  "description": "A simple React boilerplate to spin up further experiments",
  "main": "index.js",
  "author": "Trevor Pierce",
  "license": "CC0-1.0",
  "scripts": {
    "build": "npm-run-all clean webpackBuild server",
    "clean": "rm -rf dist/",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx}'",
    "start": "yarn webpackDev",
    "start:lan": "yarn webpackDev:lan",
    "server": "node server.js",
    "webpackDev": "webpack-dev-server --open --mode development --config webpack.dev.config --hot",
    "webpackDev:lan": "webpack-dev-server --open --mode development --host 0.0.0.0 --config webpack.dev.config --hot",
    "webpackBuild": "webpack --mode production --config webpack.build.config",
    "cy:run": "node scripts/cypress.js",
    "cy:open": "node_modules/.bin/cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "cypress": "^4.8.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^6.0.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "csp-html-webpack-plugin": "^3.0.3",
    "eslint-loader": "^3.0.0",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "history": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-axe": "^3.3.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-focus-lock": "^2.5.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.12",
    "react-modal": "^3.10.1",
    "react-router-dom": "^5.0.1",
    "yup": "^0.32.9"
  }
}
